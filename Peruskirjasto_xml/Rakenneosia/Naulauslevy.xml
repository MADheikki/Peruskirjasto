<Script_3D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
building_material aine
material pinnoite 
if not(varjot) then shadow off

rotx kulma
roty kallistus

if tyyppi = "Suora" then	
	block a, b, paksuus
endif
if tyyppi = "Kulma" then
	add a, b, 0
	roty -90
	rotz  -90
	prism_ 6,a,
		0,0,15,
		0,zzyzx, 15,
		paksuus, zzyzx, 15,
		paksuus, paksuus, 15,
		b, paksuus, 15,
		b,0,15
	del 3

endif

end ! -------------- end

]]>
</Script_3D>

